<h3>詳細1</h3>
<%= @list.title %>
<%= @list.body %>
<% if @list.image.attached? %>
  <%= image_tag @list.image, size: "300x300" %>
<% else %>
  <%= image_tag 'no_image', size: "300x300" %>
<% end %>
<p><%= link_to "編集", edit_list_path(@list) %></p>
<p><%= link_to "削除", list_path(@list), method: :delete, "data-confirm" => "本当に削除しますか？" %></p>